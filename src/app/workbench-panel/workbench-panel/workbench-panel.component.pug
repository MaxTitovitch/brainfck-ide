.tabs
  button((click)='clickOnTabButton(TAB_TITLES.Output)') {{TAB_TITLES.Output}}
  button((click)='clickOnTabButton(TAB_TITLES.Breakpoints)') {{TAB_TITLES.Breakpoints}}

ng-container(*ngIf='isContentOpened')
  ng-container(
    [ngSwitch]='activeTabTitle'
  )
    app-breakpoints-tab(*ngSwitchCase='TAB_TITLES.Breakpoints')
    app-output-tab(*ngSwitchCase='TAB_TITLES.Output')
